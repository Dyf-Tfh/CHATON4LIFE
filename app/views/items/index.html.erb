<div class="wrapper">
            <!-- Hero-->
            <section class="module-cover parallax" data-background="assets/images/module-15.jpg" data-overlay="0.1" style="background-image: none; z-index: 0;" data-jarallax-original-styles="background-image: url(&quot;assets/images/module-15.jpg&quot;);">
                <div class="container">
                    <div class="row">
                        <div class="col-md-12">
                            <h2>Shop Grid</h2>
                            <ol class="breadcrumb">
                                <li class="breadcrumb-item"><a href="#">Home</a></li>
